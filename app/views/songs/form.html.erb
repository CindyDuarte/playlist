<h1>Ingresa tus canciones</h1>

<br><button type="button" name="Home"><%= link_to "Volver al Home", root_path %></button><br><br>

<%= form_for @song do |f|%>
  <div class="form-group">
      <label for="song_name">Nombre de la canci√≥n</label>
        <%= f.text_field :name %>
  </div>
  <div class="form-group">
      <label for="song_url">Url</label>
        <%= f.text_field :url %>
  </div>

  <div class="form-group">
      <label for="song_artist">Artista</label>
        <%= f.text_field :artist %>
  </div>

  <div class="form-group">
      <label for="song_genre">Genero</label>
        <%= f.text_field :genre %>
  </div>

  <%= f.submit %>
<%end%>
